{   
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "parserOptions": {
      "sourceType": "module"
  }
}